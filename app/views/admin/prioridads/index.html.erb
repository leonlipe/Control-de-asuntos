<h2 class="title">Listado de prioridades </h2>
<%= link_to image_tag("icons/001_01.png", {:alt => "Nueva categoría", :size => "24x24"}), new_admin_prioridad_path %>

<div style="clear: both;">&nbsp;</div>
<div class="entry">

<table class="index">
  <tr>
	<th> Tipo </th>
  </tr>

<% @prioridads.each do |prioridad| %>
  <tr>
	<td> <%= prioridad.tipo %></td>
    <td class="actions"><%= link_to  image_tag("icons/001_38.png", {:alt => "Visualizar asunto", :size => "16x16"}), admin_prioridad_path(prioridad) %>
    <%= link_to image_tag("icons/001_45.png", {:alt => "Editar asunto", :size => "16x16"}), edit_admin_prioridad_path(prioridad) %>
    <%= link_to image_tag("icons/001_05.png", {:alt => "Borrar asunto", :size => "16x16"}), admin_prioridad_path(prioridad), :confirm => 'Está seguro?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

</div>